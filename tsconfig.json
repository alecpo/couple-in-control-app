{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "react",
      "nativewind/types"
    ],
    "typeRoots": [
      "src/@types",
      "./node_modules/@types"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "src/**/*",
    "src/@types/nativewind-env.d.ts",
    "nativewind-env.d.ts"
  ]
}